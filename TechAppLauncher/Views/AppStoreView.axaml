<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="TechAppLauncher.Views.AppStoreView">

	<DockPanel>

		<Button Command="{Binding GetAppMusicCommand}"  Content="Launch App." DockPanel.Dock="Bottom" HorizontalAlignment="Center" />
		<ListBox Items="{Binding SelectedResults}" SelectedItem="{Binding SelectedApp}" Background="#333333" Margin="0 20">
			<ListBox.ItemsPanel>
				<ItemsPanelTemplate>
					<WrapPanel />
				</ItemsPanelTemplate>
			</ListBox.ItemsPanel>
		</ListBox>
	</DockPanel>
</UserControl>
